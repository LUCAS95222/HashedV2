{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "UserMigrationsResponse",
  "type": "object",
  "required": [
    "migrations"
  ],
  "properties": {
    "migrations": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/UserMigrationsItem"
      }
    }
  },
  "definitions": {
    "UserMigrationsItem": {
      "type": "object",
      "required": [
        "block_num",
        "fail",
        "in_progress",
        "req_id",
        "success",
        "timestamp"
      ],
      "properties": {
        "block_num": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "fail": {
          "type": "integer",
          "format": "uint8",
          "minimum": 0.0
        },
        "in_progress": {
          "type": "integer",
          "format": "uint8",
          "minimum": 0.0
        },
        "req_id": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0.0
        },
        "success": {
          "type": "integer",
          "format": "uint8",
          "minimum": 0.0
        },
        "timestamp": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        }
      }
    }
  }
}
