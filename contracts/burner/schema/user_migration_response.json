{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "UserMigrationResponse",
  "type": "object",
  "required": [
    "txs"
  ],
  "properties": {
    "txs": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/TxResponse"
      }
    }
  },
  "definitions": {
    "NftExtension": {
      "type": "object",
      "required": [
        "attributes"
      ],
      "properties": {
        "animation_url": {
          "type": [
            "string",
            "null"
          ]
        },
        "attributes": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/NftExtensionDisplay"
          }
        },
        "background_color": {
          "type": [
            "string",
            "null"
          ]
        },
        "description": {
          "type": [
            "string",
            "null"
          ]
        },
        "external_url": {
          "type": [
            "string",
            "null"
          ]
        },
        "image": {
          "type": [
            "string",
            "null"
          ]
        },
        "image_data": {
          "type": [
            "string",
            "null"
          ]
        },
        "name": {
          "type": [
            "string",
            "null"
          ]
        },
        "youtube_url": {
          "type": [
            "string",
            "null"
          ]
        }
      }
    },
    "NftExtensionDisplay": {
      "type": "object",
      "properties": {
        "display_type": {
          "type": [
            "string",
            "null"
          ]
        },
        "trait_type": {
          "type": [
            "string",
            "null"
          ]
        },
        "value": {
          "type": [
            "string",
            "null"
          ]
        }
      }
    },
    "NftInfo": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "extension": {
          "anyOf": [
            {
              "$ref": "#/definitions/NftExtension"
            },
            {
              "type": "null"
            }
          ]
        },
        "id": {
          "type": "string"
        },
        "uri": {
          "type": [
            "string",
            "null"
          ]
        }
      }
    },
    "Status": {
      "type": "string",
      "enum": [
        "created",
        "swapped",
        "paid_back"
      ]
    },
    "TxResponse": {
      "type": "object",
      "required": [
        "from",
        "id",
        "status",
        "to",
        "token_addr",
        "user_req_id"
      ],
      "properties": {
        "amount": {
          "type": [
            "string",
            "null"
          ]
        },
        "from": {
          "type": "string"
        },
        "id": {
          "type": "integer",
          "format": "uint64",
          "minimum": 0.0
        },
        "minter_id": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint64",
          "minimum": 0.0
        },
        "minter_tx_hash": {
          "type": [
            "string",
            "null"
          ]
        },
        "msg": {
          "type": [
            "string",
            "null"
          ]
        },
        "nft_info": {
          "anyOf": [
            {
              "$ref": "#/definitions/NftInfo"
            },
            {
              "type": "null"
            }
          ]
        },
        "status": {
          "$ref": "#/definitions/Status"
        },
        "to": {
          "type": "string"
        },
        "token_addr": {
          "type": "string"
        },
        "user_req_id": {
          "type": "integer",
          "format": "uint32",
          "minimum": 0.0
        }
      }
    }
  }
}
